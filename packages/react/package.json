{
	"name": "uberschrift",
	"version": "1.1.13",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peerigon/uberschrift.git"
	},
	"description": "Zero-dependency magic heading levels for React",
	"scripts": {
		"build": "tsup src/index.ts --dts --clean --format cjs,esm",
		"dev": "npm run build -- --watch",
		"prepublish": "npm run build",
		"start": "npm run dev",
		"test": "run-p test:*",
		"test:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"test:scripts": "scriptlint --strict --fix",
		"test:types": "tsc --noEmit",
		"test:unit": "vitest run"
	},
	"author": "developers@peerigon.com",
	"license": "UNLICENSED",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^14.3.1",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"@uberschrift/eslint-config": "*",
		"@uberschrift/tsconfig": "*",
		"eslint": "^8.57.0",
		"jsdom": "^23.2.0",
		"npm-run-all": "^4.1.5",
		"react-dom": "^18.2.0",
		"scriptlint": "^3.0.0",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.10",
		"vitest": "^1.5.0"
	},
	"peerDependencies": {
		"react": "^18.2 || ^19.0",
		"@types/react": "*",
		"@types/react-dom": "*"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/react-dom": {
			"optional": true
		}
	}
}
