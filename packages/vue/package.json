{
	"name": "vueberschrift",
	"version": "0.1.0",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.umd.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peerigon/uberschrift.git"
	},
	"description": "Zero-dependency magic heading levels for React",
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"prepublish": "npm run build",
		"start": "npm run dev",
		"test": "run-p test:*",
		"test:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"test:scripts": "scriptlint --strict --fix",
		"test:types": "tsc --noEmit",
		"test:unit": "vitest run"
	},
	"author": "developers@peerigon.com",
	"license": "UNLICENSED",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/vue": "^8.0.3",
		"@types/vue": "^2.0.0",
		"@uberschrift/eslint-config": "*",
		"@uberschrift/tsconfig": "*",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/compiler-sfc": "^3.4.23",
		"eslint": "^8.56.0",
		"jsdom": "^23.2.0",
		"npm-run-all": "^4.1.5",
		"scriptlint": "^3.0.0",
		"semantic-release": "^23.0.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vite-plugin-dts": "^3.8.3",
		"vitest": "^1.2.0"
	},
	"peerDependencies": {
		"vue": "^3.4.21"
	}
}
